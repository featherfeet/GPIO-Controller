class Pin {
	public int pin;
	public int x;
	public int y;
	public string label;
	public int r;
	public int g;
	public int b;
	public int a;
	public int width;
	public int height;
	public Pin(int pin, int x, int y, string label, int r, int g, int b, int a, int width, int height) {
		this.pin = pin;
		this.x = x;
		this.y = y;
		this.label = label;
		this.r = r;
		this.g = g;
		this.b = b;
		this.a = a;
		this.width = width;
		this.height = height;
	}
}
